{
    "name": "rapidapi-node-sdk",
    "version": "0.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "description": "An open-source Node.js SDK for interacting with RapidAPIâ€™s diverse set of APIs, featuring built-in caching for improved performance.",
    "keywords": [
        "RapidAPI",
        "Node.js",
        "SDK",
        "API",
        "caching",
        "opensource",
        "web-services",
        "data-fetching",
        "TypeScript",
        "npm-package"
    ],
    "author": {
        "name": "Angel S. Moreno",
        "email": "angelxmoreno@gmail.com"
    },
    "license": "MIT",
    "homepage": "https://github.com/angelxmoreno/rapidapi-node-sdk",
    "bugs": {
        "url": "https://github.com/angelxmoreno/rapidapi-node-sdk/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/angelxmoreno/rapidapi-node-sdk.git"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "engines": {
        "node": ">=18",
        "yarn": ">=1.22",
        "packageManager": "yarn"
    },
    "scripts": {
        "clean": "rm -rf coverage dist node_modules yarn.lock && yarn install",
        "prepare": "husky install",
        "prebuild": "rimraf dist",
        "build": "tsc",
        "lint": "eslint . --cache",
        "lint:fix": "eslint . --cache --fix",
        "lint:markdown": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
        "lint:markdown:fix": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" --fix",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "release": "release-it --only-version"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@types/bunyan": "^1.8.11",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.16",
        "axios-mock-adapter": "^1.22.0",
        "bunyan": "^1.8.15",
        "eslint": "^8.56.0",
        "eslint-config-universe": "^12.0.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.1",
        "markdownlint-cli2": "^0.12.1",
        "pino": "^8.18.0",
        "prettier": "^3.2.4",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3",
        "winston": "^3.11.0"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "axios-date-transformer": "^1.0.3",
        "keyv": "^4.5.4",
        "murmurhash": "^2.0.1"
    },
    "eslintConfig": {
        "root": true,
        "ignorePatterns": [
            "**/dist/**"
        ],
        "extends": [
            "universe"
        ],
        "parser": "@typescript-eslint/parser",
        "overrides": [
            {
                "files": [
                    "*.json"
                ],
                "rules": {
                    "no-unused-expressions": "off"
                }
            }
        ],
        "rules": {}
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "avoid",
        "semi": true,
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 120
    },
    "lint-staged": {
        "*.{ts,json}": "eslint --fix"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "release-it": {
        "hooks": {
            "before:init": [
                "yarn lint",
                "yarn test",
                "yarn build"
            ]
        },
        "publishConfig": {
            "registry": "https://registry.npmjs.org"
        },
        "git": {
            "commit": true,
            "commitMessage": "chore: release v${version}",
            "tag": true,
            "push": true
        },
        "github": {
            "autoGenerate": true,
            "release": true
        },
        "npm": {
            "publish": true
        }
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "<rootDir>/tests/**/*.spec.ts"
        ],
        "coverageDirectory": "<rootDir>/coverage"
    },
    "markdownlintConfig": {
        "default": true,
        "MD003": {
            "style": "atx_closed"
        },
        "MD007": {
            "indent": 4
        },
        "no-hard-tabs": true,
        "whitespace": false
    }
}
